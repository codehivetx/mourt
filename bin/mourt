#!/usr/bin/env node

const Cli = require('../lib/cli');
const process = require('process');
const argv = require('minimist')(process.argv.slice(2), Cli.opts);

//console.log(argv);
// This miniimal entrypoint is kept as non-typescriptâ€¦for now?

const cli = new Cli();
cli.run(argv)
    .then(() => { },
        (err) => {
            console.error(err);
            process.exitCode = 1;
        });
